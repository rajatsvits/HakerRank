#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    long int i,j,k,l,n,ctr=0,ptr=0,q;
    cin>>n;
    long int a[n],b[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
        if(i>0&&a[i]<a[i-1])
        {
           ctr++;
            if(ctr==1)
                l=i;
            if(ctr==2)
                k=i;
            if(ctr>2)
                k=i;
        }
        
    }
   // cout<<"Ctr="<<ctr<<endl;
    //cout<<"l="<<l<<endl;
    //cout<<"k="<<k<<endl;
    if(ctr==0)
        cout<<"yes";
    else if(ctr==1&&n==2)
    {

        cout<<"yes"<<endl;
        cout<<"swap "<<l<<" "<<l+1<<endl;
    }
    else if(ctr==1)
    {  q=a[l];
       a[l]=a[l-1];
       a[l-1]=q;
     for(i=0;i<n-1;i++)
              {
                  if(a[i]>a[i+1])
                  {
                      cout<<"no";
                      goto h;
                  }
                
              }
              
                       cout<<"yes"<<endl;
                 cout<<"swap "<<l<<" "<<l+1<<endl;
     h:
     ;
       
    }
    else 
    {    
        if(ctr==2)
        {
            i=a[l-1];
            a[l-1]=a[k];
            a[k]=i;
              for(i=0;i<n-1;i++)
              {
                  if(a[i]>a[i+1])
                  {
                      cout<<"no";
                      goto c;
                  }
                
              }
              
                       cout<<"yes"<<endl;
                 cout<<"swap "<<l<<" "<<k+1<<endl;
                      
                  
            c:
            ;
        }
        else
        {
             for(i=l-1;i<=k;i++)
             {
                  b[i]=a[i];
             }
             for(i=k,j=l-1;i>=l,j<=k;i--,j++)
             {  
                 a[i]=b[j];
                  //cout<<a[i]<<endl;
             }
            
             for(i=0;i<n-1;i++)
              { // cout<<a[i]<<endl;
                  if(a[i]>a[i+1])
                  {
                      cout<<"no";
                      goto e;
                  }
                
              }
              
                       cout<<"yes"<<endl;
                 cout<<"reverse "<<l<<" "<<k+1<<endl;
                      
                  
            e:
            ;
        }
        
        
    }
    
    return 0;
}

